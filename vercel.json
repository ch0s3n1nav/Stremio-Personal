{
  "version": 2,
  "builds": [
    {
      "src": "api/manifest.json",
      "use": "@vercel/static"
    },
    {
      "src": "api/configure.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/catalog/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/manifest.json",
      "dest": "/api/manifest.json"
    },
    {
      "src": "/configure",
      "dest": "/api/configure.js"
    },
    {
      "src": "/catalog/movie/debrid-cloud.json",
      "dest": "/api/catalog/movie/debrid-cloud.json.js"
    },
    {
      "src": "/catalog/movie/ufc-events.json",
      "dest": "/api/catalog/movie/ufc-events.json.js"
    },
    {
      "src": "/catalog/series/debrid-cloud.json",
      "dest": "/api/catalog/series/debrid-cloud.json.js"
    },
    {
      "src": "/catalog/series/ufc-events.json",
      "dest": "/api/catalog/series/ufc-events.json.js"
    }
  ],
  "env": {
    "REAL_DEBRID_API_KEY": "@real_debrid_api_key"
  }
}
