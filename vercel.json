{
  "version": 2,
  "builds": [
    {
      "src": "api/manifest.json",
      "use": "@vercel/static"
    },
    {
      "src": "api/configure.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/catalog/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/meta/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/stream/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/test-player.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/test-image.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/test-tmdb.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/test-tmdb-simple.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/test-tmdb-direct.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/test-tmdb-penguin.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/debug-torrents.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/debug-catalog.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/debug-env.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/manifest.json",
      "dest": "/api/manifest.json"
    },
    {
      "src": "/configure",
      "dest": "/api/configure.js"
    },
    {
      "src": "/catalog/movie/debrid-cloud.json",
      "dest": "/api/catalog/movie/debrid-cloud.js"
    },
    {
      "src": "/catalog/movie/ufc-events.json",
      "dest": "/api/catalog/movie/ufc-events.js"
    },
    {
      "src": "/meta/movie/(.*).json",
      "dest": "/api/meta/movie/[id].js?id=$1"
    },
    {
      "src": "/stream/movie/(.*).json",
      "dest": "/api/stream/movie/[id].js?id=$1"
    },
    {
      "src": "/test-player",
      "dest": "/api/test-player.js"
    },
    {
      "src": "/test-image",
      "dest": "/api/test-image.js"
    },
    {
      "src": "/test-tmdb",
      "dest": "/api/test-tmdb.js"
    },
    {
      "src": "/test-tmdb-simple",
      "dest": "/api/test-tmdb-simple.js"
    },
    {
      "src": "/test-tmdb-direct",
      "dest": "/api/test-tmdb-direct.js"
    },
    {
      "src": "/test-tmdb-penguin",
      "dest": "/api/test-tmdb-penguin.js"
    },
    {
      "src": "/debug-torrents",
      "dest": "/api/debug-torrents.js"
    },
    {
      "src": "/debug-catalog",
      "dest": "/api/debug-catalog.js"
    },
    {
      "src": "/debug-env",
      "dest": "/api/debug-env.js"
    }
  ],
  "env": {
    "REAL_DEBRID_API_KEY": "VYESHZI4D3BF3DLJ6W4242ZTCCSR3RVSFWTGKTVRTOQ223LDRI6A",
    "TMDB_API_KEY": "8d6b19dfd39b5efe48ed79708a6850b3"
  }
}
