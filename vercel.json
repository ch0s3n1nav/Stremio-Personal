{
  "version": 2,
  "builds": [
    {
      "src": "api/manifest.json",
      "use": "@vercel/static"
    },
    {
      "src": "api/configure.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/catalog/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/meta/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/stream/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/test-player.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/utils/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/manifest.json",
      "dest": "/api/manifest.json"
    },
    {
      "src": "/configure",
      "dest": "/api/configure.js"
    },
    {
      "src": "/catalog/movie/debrid-cloud.json",
      "dest": "/api/catalog/movie/debrid-cloud.js"
    },
    {
      "src": "/catalog/movie/ufc-events.json",
      "dest": "/api/catalog/movie/ufc-events.js"
    },
    {
      "src": "/meta/movie/(.*).json",
      "dest": "/api/meta/movie/[id].js?id=$1"
    },
    {
      "src": "/stream/movie/(.*).json",
      "dest": "/api/stream/movie/[id].js?id=$1"
    },
    {
      "src": "/test-player",
      "dest": "/api/test-player.js"
    }
    {
      "src": "/test-image",
      "dest": "/api/test-image.js"
    }
  ],
  "env": {
    "REAL_DEBRID_API_KEY": "VYESHZI4D3BF3DLJ6W4242ZTCCSR3RVSFWTGKTVRTOQ223LDRI6A",
    "TMDB_API_KEY": "your_tmdb_api_key_here"
  }
}
