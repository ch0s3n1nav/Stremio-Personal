{
  "version": 2,
  "builds": [
    {
      "src": "api/manifest.json",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/manifest.json",
      "dest": "/api/manifest.json"
    },
    {
      "src": "/configure",
      "dest": "/api/configure.js"
    },
    {
      "src": "/catalog/movie/debrid-cloud.json",
      "dest": "/api/catalog/movie/debrid-cloud.js"
    },
    {
      "src": "/catalog/movie/ufc-events.json",
      "dest": "/api/catalog/movie/ufc-events.js"
    },
    {
      "src": "/meta/movie/(.*).json",
      "dest": "/api/meta/movie/[id].js?id=$1"
    },
    {
      "src": "/stream/movie/(.*).json",
      "dest": "/api/stream/movie/[id].js?id=$1"
    },
    {
      "src": "/test-player",
      "dest": "/api/test-player.js"
    },
    {
      "src": "/debug-env",
      "dest": "/api/debug-env.js"
    },
    {
      "src": "/test-tmdb-simple",
      "dest": "/api/test-tmdb-simple.js"
    },
    {
      "src": "/test-tmdb-direct",
      "dest": "/api/test-tmdb-direct.js"
    }
  ],
  "env": {
    "REAL_DEBRID_API_KEY": "WKN4Z3L4RDJVINMSNIY4EOXU657HKLA2ZKH47IHH3NJVAUBCZXFQ",
    "TMDB_API_KEY": "8d6b19dfd39b5efe48ed79708a6850b3"
  }
}
